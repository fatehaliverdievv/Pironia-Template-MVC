
@{
    ViewData["Title"] = "Index";
}
@model ICollection<ShippingArea>
<div class="table-responsive">
   <table class="table">
     <thead>
       <tr>
         <th>Id</th>
         <th>Logo</th>
         <th>Name</th>
         <th>Info</th>
       </tr>
     </thead>
     <tbody>
     @foreach (ShippingArea item in Model)
      {
         <tr>
         <td>@item.Id</td>
         <td><img width="40" src="@item.LogoUrl"></td>
         <td>@item.Name</td>
         <td>@item.Info</td>
         <td><a type="button" class="btn btn-dark" asp-area="Manage" asp-controller="ShippingArea" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
         <td><a type="button" class="btn btn-success"  asp-area="Manage" asp-controller="ShippingArea" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
         </tr>
      }
    </tbody>  
   </table>
 </div>

