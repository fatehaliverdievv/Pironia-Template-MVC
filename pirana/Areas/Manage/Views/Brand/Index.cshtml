
@{
    ViewData["Title"] = "Index";
}
@model ICollection<Brand>
<div class="table-responsive">
                    <table class="table">
                      <thead>
                        <tr>
                          <th>Id</th>
                          <th>Logo</th>
                        </tr>
                      </thead>
                      <tbody>
            @foreach (Brand item in Model)
            {
                                <tr>
                                  <td>@item.Id</td>
                                  <td><img width="40" src="~/assets/images/@item.LogoUrl"></img></td>
                                          <td><a type="button" class="btn btn-dark" asp-area="Manage" asp-controller="Brand" asp-action="Delete" asp-route-id="@item.Id">Delete</a></td>
                                              <td><a type="button" class="btn btn-success"  asp-area="Manage" asp-controller="Brand" asp-action="Update" asp-route-id="@item.Id">Update</a></td>
                                </tr>
            }
                      </tbody>  
                    </table>
                  </div>

